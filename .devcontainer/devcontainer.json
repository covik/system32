{
  "build": {
    "dockerfile": "Dockerfile",
    "target": "dev"
  },
  "postCreateCommand": "yarn install --frozen-lockfile && cp -n .env.example .env",
  "postAttachCommand": "echo '{}' > ~/.docker/config.json",
  "runArgs": ["--env-file", "${localWorkspaceFolder}/.env"],
  "mounts": [
    "source=jetbrains,target=/home/developer/.cache/JetBrains,type=volume",
    "source=pulumi,target=/home/developer/.pulumi,type=volume",
    "source=yarn,target=/var/cache/yarn,type=volume"
  ],
  "containerEnv": {
    "KUBECONFIG": "${containerWorkspaceFolder}/tmp/kubeconfig.yml"
  }
}
